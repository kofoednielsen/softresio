{
  "imports": {
    "hono": "jsr:@hono/hono@^4.11.4",
    "postgres": "npm:postgres@3.4.7"
  },
  "tasks": {
    "start": "deno serve --allow-read --allow-net --allow-sys --allow-env src/main.ts",
    "start-dev": "deno serve --allow-read --allow-net --allow-sys --allow-env --watch src/main.ts"
  },
  "fmt": {
    "semiColons": false
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
